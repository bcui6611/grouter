[ "choose mutation|read",
  [ "choose create|update",
    [ "new", "create" ],
    [ "choose delete|set",
      [ "choose hot-delete|cold-delete",
        [ "hot", "delete" ],
        [ "cold", "delete" ] ],
      [ "choose hot-set|cold-set",
        [ "hot", "set" ],
        [ "cold", "set" ] ] ] ],
  [ "choose query|primary-key",
    [ "choose hot-query|cold-query",
      [ "hot", "query" ],
      [ "cold", "query" ] ],
    [ "choose miss-get|hit-get",
      [ "miss", "get" ],
      [ "choose hot-get|cold-get",
        [ "hot", "get" ],
        [ "cold", "get" ] ] ] ] ]

