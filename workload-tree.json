{
  "ratio-mutation/read": {
    "ratio-create/update": [
      "new-create"
    ],
    "else": {
      "ratio-delete/set": {
        "ratio-hot-delete/cold-delete": [
          "hot-delete"
        ],
        "else": [
          "cold-delete"
        ]
      },
      "else": {
        "ratio-hot-set/cold-set": [
          "hot-set"
        ],
        "else": [
          "cold-set"
        ]
      }
    }
  },
  "else": {
    "ratio-query/primary-key": {
      "ratio-hot-query/cold-query": [
        "hot-query"
      ],
      "else": [
        "cold-query"
      ]
    },
    "else": {
      "ratio-miss-get/hit-get": [
        "miss-get"
      ],
      "else": {
        "ratio-hot-get/cold-get": [
          "hot-get"
        ],
        "else": [
          "cold-get"
        ]
      }
    }
  }
}
